<?xml version="1.0" encoding="utf-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" 
	xmlns:android="http://schemas.android.com/apk/res/android" 
	id="org.bar.foo" 
	version="0.0.1">
  <name>MyPluginName</name>
  <AUTHOR>someOne</AUTHOR>
  <DESCRIPTION>我就是我</DESCRIPTION>
  <js-module name="MyPluginName" src="www/MyPluginName.js">
    <clobbers target="cordova.plugins.MyPluginName"/>
  </js-module>
  <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
      <feature name="MyPluginName">
        <param name="android-package" value="org.bar.foo.MyPluginName"/>
      </feature>
    </config-file>
    <config-file parent="/*" target="AndroidManifest.xml"/>
    <source-file src="src/android/MyPluginName.java" target-dir="src/org/bar/foo/MyPluginName"/>
  </platform>
</plugin>
